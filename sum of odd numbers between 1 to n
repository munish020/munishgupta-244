/// sum of odd numbers between 1 to n ///
#include <stdio.h>

int main()
{
    int i,n,sum=0;
    printf("enter a number:");
    scanf("%d",&n);
    
    while(i<=n)
    {
        if(i%2!=0)
        {
            sum =sum+i;
        }
        i++;
    }
    printf("sum is %d",sum);
    return 0;
    
}
